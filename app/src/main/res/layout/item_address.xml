<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/conContainer"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_margin="@dimen/dp_8"
    android:background="@drawable/bg_address_selector"
    android:paddingStart="@dimen/dp_16"
    android:paddingBottom="@dimen/dp_16"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toTopOf="parent">


    <com.fameget.dreamgroceries.customviews.CFTextView

        android:id="@+id/tvAddress1"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:maxLines="5"
        android:paddingTop="@dimen/dp_16"
        android:paddingEnd="@dimen/dp_16"
        android:text="34B NewPark Street Vancouver"
        android:textColor="@color/colorAccent"
        android:textSize="@dimen/txt_14"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <com.fameget.dreamgroceries.customviews.CFTextView
        android:id="@+id/tvEdit"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:paddingTop="@dimen/dp_16"
        android:paddingEnd="@dimen/dp_16"
        android:text="@string/edit"
        android:textColor="@color/blue"
        app:layout_constraintEnd_toStartOf="@id/tvDelete"
        app:layout_constraintTop_toBottomOf="@id/tvAddress1" />

    <com.fameget.dreamgroceries.customviews.CFTextView
        android:id="@+id/tvDelete"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:paddingTop="@dimen/dp_16"
        android:paddingEnd="@dimen/dp_16"
        android:text="@string/delete"
        android:textColor="@color/red"
        app:layout_constraintBottom_toBottomOf="@id/tvEdit"
        app:layout_constraintEnd_toEndOf="parent" />


</androidx.constraintlayout.widget.ConstraintLayout>


